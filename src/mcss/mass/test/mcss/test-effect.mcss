.container{
  $effect-outport = 2;
  $fixed-container: 800px;
  div{
    $border-radius: 30px, top;
    $lh: 60px;

    float: left;
    width: 100px;
    margin-top: 30px; 
    margin-right: 40px;

    cursor: pointer;
    text-align: center;
    font-size: 20px;
    font-weight: bold;
    color: #fff;
    background-color: #16A085;
    &:hover{
      background-color: l-adjust( #16A085, 20%);
    }
    &:active{
      background-color: l-adjust( #16A085, -10%);
    }
  }
}

$font-face(hello, 'heihei', {
  font-weight: bold;
}, true);


